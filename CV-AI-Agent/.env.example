# Google AI API Configuration
GOOGLE_API_KEY=your_google_api_key_here

# Application Configuration
APP_ENV=development
DEBUG=true

# File Upload Configuration
MAX_FILE_SIZE_MB=10

# LLM Configuration
GEMINI_MODEL=gemini-2.0-flash
EMBEDDING_MODEL=models/embedding-001
LLM_TEMPERATURE=0.7
MAX_TOKENS=20000

# RAG Configuration
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
RETRIEVAL_K=5

# Directory Configuration
OUTPUT_DIR=generated_documents
TEMP_DIR=generated_documents/temp

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=application_factory.log

# Optional: Company/User Information (for file naming)
DEFAULT_COMPANY_NAME=
DEFAULT_USER_NAME=